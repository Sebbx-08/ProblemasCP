package co.edu.utadeo.cpp;
import java.util.*;
public class qaly {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		double r = 0.0;
		for (int i = 0; i < n; i++) {
			double q = sc.nextDouble();
			double y = sc.nextDouble();
			r += q * y;
		}
		System.out.println(r);
	}
}
